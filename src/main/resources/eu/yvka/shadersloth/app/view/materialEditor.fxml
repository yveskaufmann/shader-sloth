<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import eu.yvka.shadersloth.app.controls.*?>
<?import org.controlsfx.control.PropertySheet.* ?>
<?import org.controlsfx.control.PropertySheet?>

<VBox fx:id="materialEditor" fillWidth="true" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<padding>
   <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
</padding>
<children>
   <HBox spacing="5">
      <children>
         <ListView fx:id="materialList" editable="true" prefHeight="200.0" prefWidth="200.0" />
         <VBox maxHeight="25.0" prefWidth="25.0">
            <children>
               <Button fx:id="addMaterialButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" style="-fx-background-radius: 10 10 0 0;" text="+" />
               <Button fx:id="removeMaterialButton" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="25.0" prefWidth="29.0" style="-fx-background-radius: 0 0 10 10;" text="-" />
            </children>
         </VBox>
      </children>
   </HBox>
   <PropertySheet fx:id="materialProperties" VBox.vgrow="ALWAYS" maxWidth="Infinity"/>
</children>
</VBox>

